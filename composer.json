{
  "name": "fipps/isotope-colorattribute-bundle",
  "description": "Contao 4 Color Attribute Bundle",
  "keywords": [
    "Contao",
    "Isotope",
    "Color Attribute"
  ],
  "type": "contao-bundle",
  "homepage": "https://gitlab.fipps.de/contao/contao4/colorattribute-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Arne Borchert",
      "homepage": "http://www.fipps.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://gitlab.fipps.de/contao/contao4/colorattribute-bundle/issues"
  },
  "require": {
    "php": "^5.6|^7.0",
    "contao/core-bundle": "~4.4",
    "Doctrine/ORM": "^2.5",
    "doctrine/data-fixtures": "1.0.*",
    "doctrine/dbal": "~2.4",
    "isotope/isotope-core": "^2.5"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Fipps\\ColorattributeBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Fipps\\ColorattributeBundle\\ContaoManager\\Plugin"
  }
}